
---
import Layout from '../../layouts/Layout.astro';
import { getEntryBySlug } from 'astro:content';

let Content;
const entry = await getEntryBySlug('sales', 'executive_summary_feature_enablement_as_a_service');
if (entry) {
  ({ Content } = await entry.render());
}
---

<Layout title="Sales & Vision" description="Vision, executive summaries, and feature enablement strategy.">
  <section class="docs-content">
    {Content ? <Content /> : <p>Documentation not found.</p>}
  </section>
</Layout>
